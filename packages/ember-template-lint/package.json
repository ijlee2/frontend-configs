{
  "name": "@ijlee2-frontend-configs/ember-template-lint",
  "version": "1.1.0",
  "description": "Configuration for ember-template-lint",
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/frontend-configs.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "index.cjs",
  "scripts": {
    "lint": "prettier \"**/*.{cjs,mjs}\" --cache --check",
    "lint:fix": "prettier \"**/*.{cjs,mjs}\" --cache --write"
  },
  "dependencies": {
    "ember-template-lint-plugin-prettier": "^5.0.0"
  },
  "devDependencies": {
    "@ijlee2-frontend-configs/prettier": "workspace:*",
    "prettier": "^3.5.3"
  },
  "peerDependencies": {
    "ember-template-lint": "^7.7.0",
    "prettier": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "ember-template-lint": {
      "optional": false
    },
    "prettier": {
      "optional": false
    }
  },
  "engines": {
    "node": "20.* || >= 22"
  }
}

{
  "name": "my-codemod",
  "version": "1.0.7",
  "private": true,
  "description": "A Node project for testing @ijlee2-frontend-configs",
  "keywords": [
    "codemod",
    "ember-codemod",
    "emberjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/frontend-configs.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": "dist/bin/my-codemod.js",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./build.sh --production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepare": "pnpm build",
    "release:prepare": "changeset version",
    "release:publish": "pnpm build && changeset publish",
    "test": "./build.sh --test && mt dist-for-testing --quiet"
  },
  "dependencies": {
    "@codemod-utils/files": "^3.0.3",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@codemod-utils/tests": "^2.0.2",
    "@ijlee2-frontend-configs/eslint-config-node": "workspace:*",
    "@ijlee2-frontend-configs/prettier": "workspace:*",
    "@ijlee2-frontend-configs/typescript": "workspace:*",
    "@sondr3/minitest": "^0.1.2",
    "@types/node": "^22.17.0",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.2.1",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": "20.* || >= 22"
  }
}

{
  "name": "my-codemod",
  "version": "0.1.4",
  "private": true,
  "description": "A Node project for testing @ijlee2-frontend-configs",
  "keywords": [
    "codemod",
    "ember-codemod",
    "emberjs"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": "dist/bin/my-codemod.js",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./build.sh --production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepare": "pnpm build",
    "release:prepare": "changeset version",
    "release:publish": "pnpm build && changeset publish",
    "test": "./build.sh --test && mt dist-for-testing --quiet"
  },
  "dependencies": {
    "@codemod-utils/files": "^2.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@codemod-utils/tests": "^1.1.11",
    "@ijlee2-frontend-configs/eslint-config-node": "workspace:*",
    "@ijlee2-frontend-configs/prettier": "workspace:*",
    "@ijlee2-frontend-configs/typescript": "workspace:*",
    "@sondr3/minitest": "^0.1.2",
    "@types/node": "^18.19.80",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": "18.* || >= 20"
  }
}

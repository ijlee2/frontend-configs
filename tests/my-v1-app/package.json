{
  "name": "my-v1-app",
  "version": "1.0.17",
  "private": true,
  "description": "An Ember app for testing @ijlee2-frontend-configs",
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/frontend-configs.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache",
    "lint:css:fix": "stylelint \"**/*.css\" --allow-empty-input --fix",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prelint:types": "type-css-modules --src app",
    "lint:types": "ember-tsc --noEmit",
    "start": "ember serve",
    "test": "ember test --test-port 0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@ember/optional-features": "^3.0.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/broccoli-side-watch": "^1.1.0",
    "@embroider/compat": "^3.9.1",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^3.5.7",
    "@embroider/router": "^3.0.6",
    "@embroider/webpack": "^4.1.2",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/ember-tsc": "^1.1.1",
    "@glint/template": "^1.7.4",
    "@ijlee2-frontend-configs/ember-template-lint": "workspace:*",
    "@ijlee2-frontend-configs/eslint-config-ember": "workspace:*",
    "@ijlee2-frontend-configs/prettier": "workspace:*",
    "@ijlee2-frontend-configs/stylelint": "workspace:*",
    "@ijlee2-frontend-configs/typescript": "workspace:*",
    "@types/qunit": "^2.19.13",
    "autoprefixer": "^10.4.24",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.1",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.11.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.3.1",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.3.0",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.2.0",
    "ember-source": "~6.11.0",
    "ember-template-imports": "^4.4.0",
    "ember-template-lint": "7.9.3",
    "ember-test-selectors": "^7.1.0",
    "embroider-css-modules": "^3.1.7",
    "eslint": "^9.39.2",
    "loader.js": "^4.7.0",
    "my-v2-addon": "workspace:*",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.1",
    "prettier": "^3.8.1",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.5.0",
    "stylelint": "^17.3.0",
    "tracked-built-ins": "^4.1.0",
    "type-css-modules": "^2.1.4",
    "typescript": "^5.9.3",
    "webpack": "^5.105.2"
  },
  "engines": {
    "node": "22.* || >= 24"
  },
  "ember": {
    "edition": "octane"
  }
}
